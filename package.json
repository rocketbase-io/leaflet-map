{
  "name": "leaflet-element",
  "version": "0.0.16",
  "main": "dist/leaflet-element.cjs.js",
  "module": "dist/leaflet-element.esm.js",
  "unpkg": "dist/leaflet-element.umd.js",
  "browser": "dist/leaflet-element.iife.js",
  "types": "dist/leaflet-element.esm.d.ts",
  "since": 2020,
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/bennypowers/leaflet-map.git"
  },
  "keywords": [
    "LeafLet",
    "Map",
    "Web Components"
  ],
  "author": "Benny Powers <web@bennypowers.com>",
  "bugs": {
    "url": "https://github.com/bennypowers/leaflet-map/issues"
  },
  "homepage": "https://github.com/bennypowers/leaflet-map#readme",
  "scripts": {
    "prepublishOnly": "npm run build",
    "postpack": "npm run clean",
    "build": "rollup -c",
    "start": "run-p serve typecheck",
    "serve": "wds --watch --open",
    "typecheck": "tsc -w --noEmit",
    "clean": "rimraf './!(declaration).{js,d.ts,js.map}' && rimraf './mixins/*.{js,d.ts,js.map}'",
    "docs": "wca --outFile custom-elements.json",
    "lint": "run-s lint:*",
    "lint:eslint": "eslint --ext .ts,.html . --ignore-path .gitignore",
    "format": "run-s format:*",
    "format:eslint": "eslint --ext .ts,.html . --fix --ignore-path .gitignore",
    "test": "wtr --coverage",
    "test:watch": "wtr --watch"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.25",
    "@pwrs/eslint-config": "0.0.9",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@types/leaflet": "^1.5.17",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "@web/dev-server": "0.0.5",
    "@web/dev-server-esbuild": "^0.2.4",
    "@web/dev-server-import-maps": "0.0.3",
    "@web/dev-server-rollup": "^0.2.5",
    "@web/test-runner": "^0.7.17",
    "@wessberg/rollup-plugin-ts": "^1.3.4",
    "api-viewer-element": "^0.4.4",
    "concurrently": "^5.3.0",
    "deepmerge": "^4.2.2",
    "eslint": "^7.8.1",
    "eslint-plugin-html": "^6.0.3",
    "geojson": "^0.5.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.3.0",
    "lodash": "^4.17.20",
    "patch-package": "^6.2.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.10",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-smart-asset": "^2.1.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts-paths": "^1.0.5",
    "typescript": "^4.0.2",
    "web-component-analyzer": "^1.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@open-wc/dedupe-mixin": "^1.3.0",
    "@pwrs/mixins": "0.0.9",
    "leaflet": "^1.7.1",
    "lit-element": "^2.4.0",
    "resize-observer-polyfill": "^1.5.1"
  }
}
